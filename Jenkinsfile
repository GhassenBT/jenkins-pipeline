pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'test commit 1 ok ya prince update 3333333333333'
        echo 'test VS code update 1 update 2222222222'
        echo 'build started >>>>>>'
      }
    }

    stage('test') {
      steps {
        echo 'testing completed >>>>>>'
      }
    }

    stage('deploy') {
      steps {
        echo 'deployed successfully >>>>'
      }
    }
    stage('Build status') {
      steps {
        echo 'Dears, a new build has been completed successfully >>>><<<<'
      }
    }

  }
}
