pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'test commint 1'
        echo 'build started >>>>>>'
      }
    }

    stage('test') {
      steps {
        echo 'testing completed >>>>>>'
      }
    }

    stage('deploy') {
      steps {
        echo 'deployed successfully >>>>'
      }
    }

  }
}